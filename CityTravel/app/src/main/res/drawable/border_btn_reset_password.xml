<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
<item android:state_pressed="true">
    <shape>
        <solid android:color="#66CCFF">
            <corners android:radius="10dp">
                <padding android:bottom="10dp" android:left="10dp" android:right="10dp" android:top="10dp"></padding>
            </corners>
        </solid>
        <stroke android:color="#66CCFF" android:width="2dp"/>
    </shape>
</item>
<item>
    <shape>
        <solid android:color="#425df9">
            <corners android:radius="10dp">
                <padding android:bottom="10dp" android:left="10dp" android:right="10dp" android:top="10dp"></padding>
            </corners>
        </solid>
        <stroke android:color="#425df9" android:width="2dp"/>
    </shape>
</item>
</selector>